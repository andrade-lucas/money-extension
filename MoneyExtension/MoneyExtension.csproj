<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>MoneyExtensionsAndrade</PackageId>
    <Version>1.0.2</Version>
    <License>MIT</License>
    <Authors>Lucas Andrade</Authors>
    <PackageDescription>Extends to converts decimal values to cents</PackageDescription>
    <Description>Extends to converts decimal values to cents</Description>
    <RepositoryUrl>https://github.com/andrade-lucas/money-extension</RepositoryUrl>
    <PackageProjectUrl>https://github.com/andrade-lucas/money-extension</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>money;int;decimal;converter</PackageTags>
    <PackageIcon>icon.png</PackageIcon>
    <Title>MoneyExtensions</Title>
